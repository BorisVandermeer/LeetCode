{
    "files.associations": {
        "future": "cpp",
        "system_error": "cpp",
        "regex": "cpp",
        "iostream": "cpp",
        "functional": "cpp",
        "iosfwd": "cpp",
        "stack": "cpp"
    },
    "code-runner.executorMap": {
    
        "javascript": "node",
        "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
        "c": "cd $dir && gcc $fileName -o outputexec && $dir/outputexec",
        "cpp": "cd $dir && g++  -std=gnu++17 $fileName -o outputexec && $dir/outputexec",
        "python": "python -u"
    }
}